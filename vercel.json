{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" }
  ],
  "functions": {
    "api/study-28.js": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 60
    },
    "api/bibleProvider.js": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
